classDiagram
    class User {
        - String id
        - String username
        - String email
        - String password
        + login()
        + register()
    }
    class Tap {
        - String id
        - String childId
        - Date timestamp
        - String status
        + getStatus()
    }
    class Child {
        - String id
        - String userId
        - String name
        + getTaps()
    }
    User --> Child
    Child --> Tap